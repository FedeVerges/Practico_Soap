<div class="content">

    <h2 for="camara"> Camaras  </h2>
    #foreach($camara in $camaras)
    <div class="col-md-1">
        <img src=$camara>
        <label>  </label>
    </div>
   #end
    #if($error)
        <div class="alert alert-danger" role="alert">
            <h4 class="alert-heading">Se ha producido un error</h4>
            <p>$error</p>
        </div>
    #end
</div>
</div>